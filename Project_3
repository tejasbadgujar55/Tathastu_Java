import java.util.*;
class Fact
{
   public static void main(String args[])
   {
       int n,fact=1;
       Scanner s=new Scanner(System.in);
       System.out.println("enter the number");
       n=s.nextInt();
       while(n>0)
       {
          fact=fact*n;
          n--;
       }
        System.out.println("Factorial is"+fact);
      }
   }   
